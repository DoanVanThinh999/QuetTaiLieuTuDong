$(document).on('click','.backdrop__body-backdrop___1rvky.active, .cart_btn-close', function() {   
	$('.backdrop__body-backdrop___1rvky, .cart-sidebar, #popup-cart-desktop, .popup-cart-mobile').removeClass('active');
